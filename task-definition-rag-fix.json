{
  "family": "myaigist-qa-task",
  "taskRoleArn": "arn:aws:iam::039357025951:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::039357025951:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "myaigist-qa-container",
      "image": "039357025951.dkr.ecr.us-east-1.amazonaws.com/myaigist:918b786f85819524e77f797119e15993eb2e5ffb",
      "portMappings": [
        {
          "containerPort": 8000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "FLASK_ENV",
          "value": "production"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/myaigist-qa",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
